      <real_time_factor>1</real_time_factor>
      <real_time_update_rate>100</real_time_update_rate>
    </physics>
    <scene>
      <ambient>0.1 0.1 0.2 1</ambient>
      <background>0.05 0.05 0.2 1</background>
      <shadows>0</shadows>
      <grid>0</grid>
    </scene>

